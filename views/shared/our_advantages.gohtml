{{define "our_advantages"}}

<div class="divider-wrapper">{{template "hr" .}}</div>
    <h3 class="text-center strong">НАШИ ПРЕИМУЩЕСТВА</h3>
    <div class="row advantages">
        {{range ourAdvantages}}
            <div class="col-sm-6 col-md-3 text-center">
                <h4 class="text-danger strong">{{.Title}}</h4>
                <div class="description">
                    {{noescape .Description}}
                </div>
            </div>
        {{end}}
    </div>
{{end}}